{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "MicroPython (Emulator)",
            "type": "debugpy",
            "request": "launch",
            "program": "/Users/christianhein/.vscode/extensions/benpaddlejones.pico-bridge-2.0.2/emulator/mock/runner.py",
            "args": [
                "${file}"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "MICROPYTHON_MOCK": "1",
